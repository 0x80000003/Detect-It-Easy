meta("protector", "NQShield");

function detect() {
    bDetected =
        APK.isArchiveRecordPresent("assets/libnqshieldx86.so") ||
        APK.isArchiveRecordPresent("assets/nqdata") ||
        APK.isArchiveRecordPresent("lib/armeabi/libnqshield.so");

    return result();
}