meta("protector", "OLLVM-TLL");

function detect() {
    bDetected =
        APK.isArchiveRecordPresent("lib/armeabi-v7a/libmtprotect.so") ||
        APK.isArchiveRecordPresent("lib/x86/libmtprotect.so");

    return result();
}