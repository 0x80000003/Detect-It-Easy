init("cruncher", "Defjam");

function detect() {
    if (Amiga.compareEP("4df9........7e..303c....3d40....3d40")) {
        sVersion = "3.2-3.6 Pro";
        bDetected = true;
    } else if (Amiga.compareEP("....43fa....4bf9........287a....204c")) {
        sVersion = "3.6";
        bDetected = true;
    }

    return result();
}