init("joiner", "ETA Text Linker");

function detect() {
    if (Amiga.compareEP("48e7fffe41fa....59..2010e5..58..43fa....2340....2c78....43fa")) {
        sVersion = "0.91-0.99";
        bDetected = true;
    } else if (Amiga.compareEP("48e7fffe41fa....59..2010e5..58..43fa....2340....203a....53..43fa")) {
        sVersion = "1.00";
        bDetected = true;
    }

    return result();
}