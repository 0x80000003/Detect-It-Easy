meta("compiler", "HiSoft BASIC");

function detect() {
    if (Amiga.compareEP("267c........6000$$$$7a..383c....263c........3e3c....243c")) {
        sOptions = "1988-94";
        bDetected = true;
    }

    sLang = "BASIC";

    return result();
}