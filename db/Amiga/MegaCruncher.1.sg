        sVersion = "1.0-1.2";
        sOptions = "Address";
        bDetected = true;
    } else if (Amiga.compareEP("48e7fffe2c78....4bfa....4cdd00034eae....4a8067")) {
        sVersion = "1.2";
        bDetected = true;
    }

    return result();
}