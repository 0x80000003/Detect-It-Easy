meta("cruncher", "ProPack (RNC)");

function detect() {
    if (Amiga.compareEP("487a....48e7fffe487a....203c........72..2c78....4eae")) {
        sOptions = "Mode 1";
        bDetected = true;
    } else if (Amiga.compareEP("487a....48e7fffe487a....2a574a956700....2a55dbcddbcd")) {
        sOptions = "Key Mode 2";
        bDetected = true;
    }

    return result();
}