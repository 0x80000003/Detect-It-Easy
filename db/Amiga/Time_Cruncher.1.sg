init("cruncher", "Time Cruncher");

function detect() {
    if (Amiga.compareEP("41fa....d1fc........43f9........2460d5c9202072..6100")) {
        sVersion = "1.7-2.2";
        bDetected = true;
    } else if (Amiga.compareEP("48e7fffe2c79........43fa....4eae....2c404eae....2200243c")) {
        sVersion = "4.2";
        sOptions = "by RAZOR and NETWORK's";
        bDetected = true;
    }

    return result();
}