init("cruncher", "Turtle Smasher");

function detect() {
    if (Amiga.compareEP("41fa....225845fa....24982449d5d8d1d041e8....201072..6100")) {
        sVersion = "1.3-1.41";
        bDetected = true;
    } else if (Amiga.compareEP("43fa....2459204a323c....429851c9....204a45ea....214a....217c")) {
        sVersion = "2.00";
        bDetected = true;
    }

    return result();
}