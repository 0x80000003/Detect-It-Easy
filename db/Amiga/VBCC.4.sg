// Ref: https://en.wikipedia.org/wiki/Vbcc

init("compiler", "VBCC");

function detect() {
    if (Amiga.compareEP("60..'VBCC '") && Amiga.compareEP("60$$2400244849f90000....294f....2c78....294e")) {
        sVersion = Amiga.getString(Amiga.getEntryPointOffset() + 7, 3);
        bDetected = true;
    }

    sLang = "C";

    return result();
}