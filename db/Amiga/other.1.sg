meta("other", "other");

function detect() {
    if (Amiga.compareEP("70..4e75'WHDLOADS'")) {
        sName = "WHDLOAD object";
        bDetected = true;
    } else if (Amiga.compareEP("70..4e75'AUTOSCPP'")) {
        sName = "SCPP object";
        bDetected = true;
    }

    return result();
}