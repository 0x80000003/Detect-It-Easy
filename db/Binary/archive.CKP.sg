// This Script supports following formats: CKP
// Used by Adiboo & Paziral's Secret

init("archive", "CKP");

function detect() {
    if (Binary.compare("'.CKP'")) {
        sVersion = "CKP";
        bDetected = true;   // Mark as detected
    }

    return result(); // Return the result of the detection
}
