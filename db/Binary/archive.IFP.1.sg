// This Script supports following formats: IFP
// Used by Grand Theft Auto San Andreas

meta("archive", "Animation file");

function detect() {
    if (Binary.compare("'ANP3'")) {
        sVersion = "3";
        bDetected = true;
    }

    return result();
}