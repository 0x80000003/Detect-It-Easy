meta("archive", "System Software Update Packages");

function detect() {
    if (Binary.compare("7F 50 4B 47")) {
        var contentID = Binary.getString(48, 48).trim();

        if (contentID) {
            sOptions = "ContentID: " + contentID;
            bDetected = true;
        }
    }

    return result();
}
