init("archive", "Playstation Update Package");

function detect() {
    if (Binary.compare("'SCEUF'")) {
        sVersion = "PS3";
        bDetected = true;
    } else if (Binary.compare("'SLB2'")) {
        sVersion = "PS4";
        bDetected = true;
    }

    return result();
}