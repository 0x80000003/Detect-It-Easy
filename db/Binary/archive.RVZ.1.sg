// https://github.com/dolphin-emu/dolphin/blob/500728360cd177466f699962b441363cd1dd9ed7/docs/WiaAndRvz.md
meta("archive", "RVZ");

function detect() {
    if (Binary.compare("'RVZ'")) {
        var gameID = Binary.getString(88, 6).trim();

        if (gameID) {
            sOptions = "GameID: " + gameID;
            bDetected = true;
        }
    }

    return result();
}
