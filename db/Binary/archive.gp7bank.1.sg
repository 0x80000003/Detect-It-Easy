// Detect It Easy: detection rule file
// Author: Kaens TG@kaens

init("archive", "Arobas Music Guitar Pro instrument (.gp7bank)");

function detect() {
  if (!X.c("'AMARCHIVE'..000000") || !X.c("00'Samples.xml'00", 0x24)) return false;
  bDetected = true; sVersion = 'v'+X.U8(9);
  if(X.isVerbose()) sOptions = X.U16(0xD)+'samples';
  return result();
}