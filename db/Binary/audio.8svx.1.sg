// Detect It Easy signature file
// Author: KDSS-Research

init("audio", "8SVX");
includeScript("read");

function detect(bShowType, bShowVersion, bShowOptions) {
    filesize = Binary.getSize();
    if (Binary.findString(0,filesize,"BODY") != -1 && Binary.findString(0,filesize,"FORM") != -1  && Binary.findString(0,filesize,"ANNO") != -1) bDetected = 1;
	

    return result(bShowType, bShowVersion, bShowOptions);
}