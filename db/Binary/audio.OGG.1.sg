// https://file-recovery.com/ogg-signature-format.htm
meta("audio", "Vorbis");

function detect() {
    if (Binary.compare("'OggS'")) {
        var vorbis = Binary.getString(29, 6).trim();

        if (/^[a-z]{6}/.test(vorbis)) {
            if (X.isVerbose()) SOptions = vorbis;
            bDetected = true;
        }
    }
        return result();
}
