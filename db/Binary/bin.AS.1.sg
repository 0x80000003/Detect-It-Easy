meta("format", "AS");

function detect() {
    if (Binary.compare("'Act;'")) {
        var actor = Binary.getString(4, 5).trim();

        if (/^[a-zA-Z/]{5}/.test(actor)) {
            if (X.isVerbose()) sOptions = actor;
            bDetected = true;
        }
    }

    return result();
}