// Used by Nintendo Wii - BackupMii/BootMii

// https://github.com/DarkMatterCore/xyzzy-mod/blob/a5139f246c4d2602c07abc0301e5917106fa4183/source/xyzzy.c#L275
init("format", "BackupMii");

function detect() {
    if (Binary.compare("'BackupMii v1'")) {
        sVersion = "v1.0";
        bDetected = true;
    }

    return result();
}
