meta("format", "CloneCD Control");

function detect() {
    if (Binary.compare("'[CloneCD]'")) {
        var versionNumber = Binary.getString(19, 1).trim();

        if (/^[0-9]{1}/.test(versionNumber)) {
            if (X.isVerbose()) sVersion = versionNumber;
            bDetected = true;
        }

    }

    return result();
}