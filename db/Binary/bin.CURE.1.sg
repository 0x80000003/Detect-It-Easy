// https://wiki.scummvm.org/index.php?title=Gob/DEV7_Information#File_Formats
meta("format", "Mesh configuration (.CURE)");

function detect() {
    if (Binary.compare("'[Global]'")) {
        var name = Binary.getString(15, 19).trim();

        if (/^[a-zA-Z0-9\_]{19}/.test(name)) {
            if (X.isVerbose()) sOptions = name;
            bDetected = true;
        }
    }

    return result();
}