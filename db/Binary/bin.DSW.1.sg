meta("format", "Microsoft Developer Studio Workspace");

function detect() {
    if (Binary.compare("'Microsoft Developer Studio Workspace File'")) {
        var versionNumber = Binary.getString(57, 5).trim();

        if (/^\d+\.\d{2}$/.test(versionNumber)) {
            sVersion = "Version: " + versionNumber;
            bDetected = true;
        }
    }

    return result();
}