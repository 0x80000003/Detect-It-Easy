// Detect It Easy: detection rule file
// Author: BJNFNE <bjnfne@web.de>

meta("format", "FAS");

function detect() {
    if (Binary.compare("'fas'")) {
        var filePath = Binary.getString(64, 32).trim();

        if (/^([\d\D]*)$/.test(filePath)) {
            if (X.isVerbose()) sOptions = "File path: " + filePath;
            bDetected = true;
        }
    }

    return result();
}
