meta("format", "FNX");

function detect() {
    if (Binary.compare("'adgames'")) {
        var copyrightInfo = Binary.getString(13, 8).trim();

        if (copyrightInfo) { // REWRITE CHECK!!!
            sVersion = copyrightInfo;
            bDetected = true;
        }
    }
    return result();
}
