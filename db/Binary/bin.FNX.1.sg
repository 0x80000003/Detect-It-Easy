// Author: BJNFNE <bjnfne@web.de>
// This Script supports following formats: FNX
// Used by Iron Willy

meta("format", "FNX");

function detect() {
    if (Binary.compare("'adgames'")) {
        var copyright = Binary.getString(13, 8).trim();

        if (copyright) {
            sVersion = "Copyright: " + copyright;
            bDetected = true;
        }
    }
    return result();
}
