// https://learn.microsoft.com/en-us/answers/questions/4116358/gamingroot-and-xboxgames-keeps-generating-even-if
init("format", "GamingRoot");

function detect() {
    if (Binary.compare("'RGBX'")) {
        sName = "Xbox Games";
        bDetected = true;
    }

    return result();
}
