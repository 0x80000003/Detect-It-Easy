meta("format", "GamingRoot");

function detect() {
    if (Binary.compare("52 47 42 58 01 00 00 00 58 00 62 00 6F 00 78 00 47 00 61 00 6D 00 65 00 73 00 00 00")) {
        sOptions = "Xbox Games";
        bDetected = true;
    }

    return result();
}