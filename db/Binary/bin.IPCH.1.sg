// Detect It Easy: detection rule file
// Author: BJNFNE <bjnfne@web.de>

init("format", "Intellisense Precompiled Header File (.IPCH)");

function detect() {
    if (Binary.compare("'EDG C/C++ version 6.6'")) {
        sVersion = "v6.6";
        bDetected = true;
    } else if (Binary.compare("'EDG C/C++ version 6.7'")) {
        sVersion = "v6.7";
        bDetected = true;
    }

    return result();
}
