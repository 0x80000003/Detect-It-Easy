// https://wiki.multimedia.cx/index.php/Duck_IVF
meta("format", "Duck IVF");

function detect() {
    if (Binary.compare("'DKIF'")) {
        var codecInfo = Binary.getString(8, 11).trim();

        if (codecInfo) {
            sOptions = "Codec: " + codecInfo;
            bDetected = true;
        }
    }
    return result();
}