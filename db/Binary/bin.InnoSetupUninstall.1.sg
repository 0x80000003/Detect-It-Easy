// Detect It Easy: detection rule file
// Author: BJNFNE <bjnfne@web.de>
// Used by Inno Setup

meta("format", "Inno Setup Uninstall Log");

function detect() {
    if (Binary.compare("'Inno Setup Uninstall Log (b) 64-bit'")) {
        sVersion = "64-bit";
        bDetected = true;
    
        var uuid = Binary.getString(64, 39).trim();
        if (uuid) {
            sOptions = "UUID: " + uuid;
            bDetected = true;
        }
    }

    return result();
}
