// Detect It Easy: detection rule file
// Author: BJNFNE <bjnfne@web.de>
// Used in Lego Island Xtreme Stunts

meta("format", "LIXS");

function detect() {
    if (Binary.compare("'Island Xtreme Stunts - Silicon Dreams'")) {
        bDetected = true;

        var copyrightYear = Binary.getString(40, 5).trim();

        if (/^[12]\d{3}$/.test(copyrightYear)) {
            sVersion = copyrightYear;
            bDetected = true;
        }
    }

    return result();
}