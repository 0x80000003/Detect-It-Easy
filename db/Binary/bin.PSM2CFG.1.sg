meta("format", "PSM2CFG");

function detect() {
    if (Binary.compare("'PSM2CFG'")) {
        var versionNumber = Binary.getString(17, 4).trim();

        if (versionNumber) {
            sVersion = "Version: " + versionNumber;
            bDetected = true;
        }
    }
    return result();
}
