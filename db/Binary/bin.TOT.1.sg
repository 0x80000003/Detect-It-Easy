init("format", "TOT");

function detect() {
    if (Binary.compare("?? **'") && Binary.isSignaturePresent(0, 32, "00 00 '(c)'")) {
        bDetected = true;

        var copyrightYear = Binary.findSignature(0, 64, "'19' %% %%");

        if (copyrightYear) {
            sVersion = Binary.getString(copyrightYear, 4);
        }
    }

    return result();
}
