meta("format", "XCI");

function detect() {
    if (Binary.compare("'xci File'")) {
        var versionNumber = Binary.getString(9, 4).trim();

        if (/^[A-Z0-9]{4}$/) {
            sOptions = "Version: " + versionNumber;
            bDetected = true;
        }
    }
    return result();
}