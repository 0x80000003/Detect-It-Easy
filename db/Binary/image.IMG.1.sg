// https://wiki.scummvm.org/index.php?title=Gob/DEV7_Information#Images
meta("image", "Image (DEV7)");

function detect() {
    if (Binary.compare("'IMG10'")) {
        sVersion = "v1.0";
        bDetected = true;
    } else if (Binary.compare("'IMG1.0.1'")) {
        sVersion = "v1.0.1";
        bDetected = true;
    }

    return result();
}