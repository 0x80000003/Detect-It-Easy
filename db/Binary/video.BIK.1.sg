// This Script supports following formats: BIK

init("format", "BIK");

function detect() {
    if (Binary.compare("'BIKi'")) {
        sVersion = "Bink Video";
        bDetected = true;   // Mark as detected
    }

    return result(); // Return the result of the detection
}
