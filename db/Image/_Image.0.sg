// Detect It Easy: detection rule file

meta("format", "Image");

function detect() {
    if (Image.isVerbose()) {
        sName = Image.getFileFormatName();
        sVersion = Image.getFileFormatVersion();
        sOptions = Image.getFileFormatOptions();
        
        if (sName) {
            bDetected = true;
        }
    }

    return result();
}