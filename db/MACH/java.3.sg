// Detect It Easy: detection rule file format

init("library", "Java Runtime");

function detect() {
    if (MACH.isLibraryPresent("libjava.dylib")) {
        bDetected = true;
    } else if (MACH.isLibraryPresent("libjvm.dylib")) {
        bDetected = true;
    }

    _setLang("Java", bDetected);}