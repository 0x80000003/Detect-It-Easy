// Detect It Easy: detection rule file

meta("library", "Java Runtime");

function detect() {
    if (MACH.isLibraryPresent("libjava.dylib")) {
        bDetected = true;
    } else if (MACH.isLibraryPresent("libjvm.dylib")) {
        bDetected = true;
    }

    sLang = "Java";

    return result();
}