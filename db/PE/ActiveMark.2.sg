// Detect It Easy: detection rule file

meta("protector", "ActiveMARK");

function detect() {
    if (PE.compareOverlay("00'TMSAMVOH'")) {
        sVersion = "5.X";
        bDetected = true;
    } else if (PE.compareEP("8925........EB")) {
        bDetected = true;
    }

    return result();
}