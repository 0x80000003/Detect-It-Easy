// https://www.nuget.org/packages/AWSSDK.SimpleEmail
meta("library", "Amazon SimpleEmail");

function detect() {
    if (PE.isNetObjectPresent("AWSSDK.SimpleEmail")) {
        sVersion = "2, 3.X-4.X";
        bDetected = true;
    }

    return result();
}