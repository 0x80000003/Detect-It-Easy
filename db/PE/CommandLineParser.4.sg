// https://github.com/commandlineparser/commandline
meta("library", "CommandLineParser");

function detect() {
    if (PE.isNetObjectPresent("CommandLine") && PE.isNetObjectPresent("ParseArguments")) {
        bDetected = true;
    }

    return result();
}