// Detect It Easy: detection rule file format

init("protector", "Enigma");

function detect() {
    if (".enigma" == PE.getSectionNameCollision("1", "2")) {
        bDetected = true;
    } else if (PE.isNET() && PE.isSignatureInSectionPresent(0, "'[enigmamasks'")) {
        bDetected = true;
    }

    if (bDetected) {
        sVersion = "Virtual Box";
    }

    return result();
}