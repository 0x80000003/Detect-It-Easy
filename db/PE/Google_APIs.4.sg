// https://github.com/googleapis/google-api-dotnet-client
meta("library", "Google APIs");

function detect() {
    if (PE.isNetObjectPresent("Google.Apis") || PE.isNetObjectPresent("Google.Apis.Auth") || PE.isNetObjectPresent("Google.Apis.Core")) {
        bDetected = true;
    }

    return result();
}