// https://en.wikipedia.org/wiki/Integrated_Performance_Primitives
meta("library", "Intel IPP");

function detect() {
    if (PE.isSectionNamePresent("IPPCODE") && PE.isSectionNamePresent("IPPDATA")) {
        bDetected = true;
    }

    return result();
}