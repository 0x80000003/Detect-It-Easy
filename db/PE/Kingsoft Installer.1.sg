init("installer","Kingsoft Corp.");

function detect(bShowType,bShowVersion,bShowOptions)
{
    if((PE.compareEP("E8")) || (PE.compareEP("60")) ) // MSV C++ or UPX
    {
        if(PE.compareOverlay("'zzd.'"))
        {
            sOptions="7z archive";
            bDetected=1;
        }
    }

    return result(bShowType,bShowVersion,bShowOptions);
}
