// https://www.nuget.org/packages/LibVLCSharp
meta("library", "LibVLCSharp");

function detect() {
    if (PE.isNetObjectPresent("LibVLCSharp") || PE.isNetObjectPresent("LibVLCSharp.Shared")) {
        bDetected = true;
    }

    return result();
}