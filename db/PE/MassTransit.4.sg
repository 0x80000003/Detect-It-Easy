// https://github.com/MassTransit/MassTransit
meta("library", "MassTransit");

function detect() {
    if (PE.isNetObjectPresent("MassTransit")) {
        bDetected = true;
    }

    if (PE.isNetObjectPresent("MassTransit.AmazonSQS")) {
        sVersion = "AmazonSQS";
        bDetected = true;
    }

    return result();
}