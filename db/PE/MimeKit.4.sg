// https://github.com/jstedfast/MimeKit
init("library", "MimeKit");

function detect() {
    if (PE.isNetObjectPresent("MimeKit")) {
        if (PE.isNetObjectPresent("MimeKitLite")) {
            sVersion = "Lite";
        }

        bDetected = true;
    }

    return result();
}