// https://openiddict.com/
meta("library", "OpenIddict");

function detect() {
    if (PE.isNetObjectPresent("OpenIddict.Client") || PE.isNetObjectPresent("OpenIddict.Abstractions")) {
        bDetected = true;
    }

    return result();
}