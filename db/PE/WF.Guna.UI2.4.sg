meta("library", "Guna UI");

function detect() {
    if (PE.isNetObjectPresent("Guna.UI2") && PE.isNetObjectPresent("Guna.UI2.WinForms")) {
        sVersion = "2.0";
        bDetected = true;
    }

    return result();
}