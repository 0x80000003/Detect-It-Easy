// https://github.com/xunit/xunit
meta("library", "xUnit.net");

function detect() {
    if (PE.isNetObjectPresent("xunit.core") || PE.isNetObjectPresent("xunit.assert") || PE.isNetObjectPresent("xunit.analyzers")) {
        bDetected = true;
    }

    return result();
}