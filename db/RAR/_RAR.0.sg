// Detect It Easy: detection rule file

meta("format", "RAR");

function detect() {
    if (RAR.isVerbose()) {
        sName = RAR.getFileFormatName();
        sVersion = RAR.getFileFormatVersion();
        sOptions = RAR.getFileFormatOptions();
        
        if (sName) {
            bDetected = true;
        }
    }

    return result();
}
