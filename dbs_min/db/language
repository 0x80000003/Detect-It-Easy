function _isLangPresent(e){return _isResultPresent("language",e)||_isResultPresent("~language",e)}function _isLangDetected(){return _getNumberOfResults("language")||_getNumberOfResults("~language")}function _setLang(e,n,s){if(1==n||null==n){if(!_isLangPresent("C/C++")||"C"!=e&&"C++"!=e||_removeResult("language","C/C++"),"C/C++"==e&&(_isLangPresent("C")||_isLangPresent("C++")))return null
if(_isLangPresent("C")&&"C"!==e&&_removeResult("language","C"),_isLangPresent("C++")&&"C++"!==e&&_removeResult("language","C++"),_isLangDetected()&&("C/C++"==e||"C++"==e||"C"==e))return null
if(File.isVerbose()&&!_isLangPresent(e)){var t="HEUR"==s
_setResult((t?"~":"")+"language",e,s&&!t?s:"","")}}}