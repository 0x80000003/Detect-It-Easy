function xma2_parse_xma2_chunk(r){for(var U=X.U8(r),_=X.U8(r+1),a=X.U32(r+4,_BE),n=X.U32(r+8,_BE),e=0<X.U8(r+3)||n,t=X.U32(r+12,_BE),u=X.U32(r+(3==U?20:28),_BE),c=0,f=3==U?32:40,i=0;i<_;i++)c+=X.U8(r+f+4*i)
return[c,t,e,u,a,n]}function xma2_parse_fmt_chunk_extra(r,U){var _=X.U32(r+24,U),a=X.U32(r+40,U),U=X.U32(r+44,U),r=X.U8(r+48)||U
return[r=a+128-512==0&&_+128-512<=U+128-512+256?!1:r,_,a,U]}includeScript("read")
